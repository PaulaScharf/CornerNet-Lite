{
    "system": {
        "dataset": "CUSTOM",
        "batch_size": 48,
        "sampling_function": "cornernet_saccade",

        "train_split": "train",
        "val_split": "val",

        "learning_rate": 0.00025,
        "decay_rate": 10,

        "val_iter": 100,

        "opt_algo": "adam",
        "prefetch_size": 5,

        "max_iter": 5000,
        "stepsize": 4000,
        "snapshot": 1000,

        "chunk_sizes": [48]
    },
    
    "db": {
        "rand_scale_min": 0.5,
        "rand_scale_max": 1.1,
        "rand_scale_step": 0.1,
        "rand_scales": null,

        "rand_full_crop": true,
        "gaussian_bump": true,
        "gaussian_iou": 0.5,

        "min_scale": 16,
        "view_sizes": [],

        "height_mult": 31,
        "width_mult": 31,

        "input_size": [765,765],
        "output_sizes": [[128, 128]],

        "att_max_crops": 30,
        "att_scales": [[1, 2, 4]],
        "att_thresholds": [0.3],
        "att_sizes": [[48, 48], [96, 96], [192, 192]],
        "att_ranges": [[288, 766], [96, 288], [0, 96]],

        "top_k": 12,
        "num_dets": 12,
        "categories": 80,
        "ae_threshold": 0.3,
        "nms_threshold": 0.5,

        "max_per_image": 100
    }
}
